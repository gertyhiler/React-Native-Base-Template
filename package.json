{
  "name": "weather-app",
  "version": "1.0.0",
  "main": "/App.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android:release": "expo run:android --variant release",
    "android:build:apk": "npm run android:prebuild:clear && cd android && ./gradlew assembleRelease",
    "android:build:aab": "npm run android:prebuild:clear && cd android && ./gradlew bundleRelease",
    "android:prebuild:clear": "expo prebuild --platform android --clean && cp local.properties android/local.properties"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@react-native-async-storage/async-storage": "1.18.2",
    "expo": "~49.0.15",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "immer": "^10.0.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.11.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-vector-icons": "^10.0.2",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-native-vector-icons": "^6.4.17",
    "@babel/core": "^7.20.0",
    "obfuscator-io-metro-plugin": "^2.1.3",
    "typescript": "^5.2.2"
  },
  "private": true
}
